<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_high_score"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:weightSum="1"
    android:background="@drawable/nen"
    tools:context="com.example.toanncth.quizapp.HighScoreActivity">

    <TextView
        android:id="@+id/textGameOver"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:layout_weight="0.230"
        android:gravity="center"

        android:text="HIGH SCORE"
        android:textColor="#F87431"
        android:textSize="35sp"
        android:textStyle="bold"
        android:shadowColor="#7000"
        android:shadowDx="12"
        android:shadowDy="12"
        android:shadowRadius="8"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal"
        android:layout_weight="0.08"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        >

        <TextView
            android:id="@+id/textView1"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:text="Khoa Học Xã hội"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="20dp"
            android:textStyle="bold"
            android:shadowColor="#7000"
            android:shadowDx="12"
            android:shadowDy="12"
            android:shadowRadius="8"
            />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:text="0"

            android:textSize="20dp"
            android:textStyle="bold"
            android:textColor="@color/colorPrimaryDark"
            android:shadowColor="#7000"
            android:shadowDx="12"
            android:shadowDy="12"
            android:shadowRadius="8"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal"
        android:layout_weight="0.08"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        >

        <TextView
            android:id="@+id/textView3"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:text="Toán Học"
            android:textColor="@color/colorAccent"
            android:textSize="20dp"
            android:textStyle="bold"
            android:shadowColor="#7000"
            android:shadowDx="12"
            android:shadowDy="12"
            android:shadowRadius="8"/>

        <TextView
            android:id="@+id/textView4"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:text="0"
            android:textColor="@color/colorAccent"
            android:textSize="20dp"
            android:textStyle="bold"
            android:shadowColor="#7000"
            android:shadowDx="12"
            android:shadowDy="12"
            android:shadowRadius="8"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal"
        android:layout_weight="0.08"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:text="Văn Học - Lịch Sử"
            android:textColor="#817339"
            android:textSize="20dp"
            android:textStyle="bold"
            android:shadowColor="#7000"
            android:shadowDx="12"
            android:shadowDy="12"
            android:shadowRadius="8"/>

        <TextView
            android:id="@+id/textView6"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:text="0"
            android:textColor="#817339"
            android:textSize="20dp"
            android:textStyle="bold"
            android:shadowColor="#7000"
            android:shadowDx="12"
            android:shadowDy="12"
            android:shadowRadius="8"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal"
        android:layout_weight="0.08"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:text="Địa Lý"
            android:textColor="#E42217"
            android:textSize="20dp"
            android:textStyle="bold"
            android:shadowColor="#7000"
            android:shadowDx="12"
            android:shadowDy="12"
            android:shadowRadius="8"/>

        <TextView
            android:id="@+id/textView8"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:text="0"
            android:textColor="#E42217"
            android:textSize="20dp"
            android:textStyle="bold"
            android:shadowColor="#7000"
            android:shadowDx="12"
            android:shadowDy="12"
            android:shadowRadius="8"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal"
        android:layout_weight="0.08"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        >

        <TextView
            android:id="@+id/textView9"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:text="Câu Đố Vui"
            android:textColor="#817679"
            android:textSize="20dp"
            android:textStyle="bold"
            android:shadowColor="#7000"
            android:shadowDx="12"
            android:shadowDy="12"
            android:shadowRadius="8"/>

        <TextView
            android:id="@+id/textView10"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:text="0"
            android:textColor="#817679"
            android:textSize="20dp"
            android:textStyle="bold"
            android:shadowColor="#7000"
            android:shadowDx="12"
            android:shadowDy="12"
            android:shadowRadius="8"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal"
        android:layout_weight="0.08"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:text="Câu Đố Mẹo"
            android:textColor="#827B60"
            android:textSize="20dp"
            android:textStyle="bold"
            android:shadowColor="#7000"
            android:shadowDx="12"
            android:shadowDy="12"
            android:shadowRadius="8"/>

        <TextView
            android:id="@+id/textView12"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:text="0"
            android:textColor="#827B60"
            android:textSize="20dp"
            android:textStyle="bold"
            android:shadowColor="#7000"
            android:shadowDx="12"
            android:shadowDy="12"
            android:shadowRadius="8"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal"
        android:layout_weight="0.08"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:id="@+id/textView13"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:text="Câu Đố Hay"
            android:textColor="#EE9A4D"
            android:textSize="20dp"
            android:textStyle="bold"
            android:shadowColor="#7000"
            android:shadowDx="12"
            android:shadowDy="12"
            android:shadowRadius="8"/>

        <TextView
            android:id="@+id/textView14"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:text="0"
            android:textColor="#EE9A4D"
            android:textSize="20dp"
            android:textStyle="bold"
            android:shadowColor="#7000"
            android:shadowDx="12"
            android:shadowDy="12"
            android:shadowRadius="8"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal"
        android:layout_weight="0.08"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:id="@+id/textView15"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:text="Câu Đố Cười"
            android:textColor="#827839"
            android:textSize="20dp"
            android:textStyle="bold"
            android:shadowColor="#7000"
            android:shadowDx="12"
            android:shadowDy="12"
            android:shadowRadius="8"/>

        <TextView
            android:id="@+id/textView16"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_margin="10dp"
            android:text="0"
            android:textColor="#827839"
            android:textSize="20dp"
            android:textStyle="bold"
            android:shadowColor="#7000"
            android:shadowDx="12"
            android:shadowDy="12"
            android:shadowRadius="8"/>
    </LinearLayout>

</LinearLayout>